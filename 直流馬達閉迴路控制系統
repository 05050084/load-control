clear all;
close all;

La=1e-3;
Ra=30;
K=50e-3;
Jm=40e-6;
Bm=35e-6;

Kp=10;
Ki=0.1;
Kd=50;

S1=tf([K],[La Ra]);
S2=tf([1],[Jm Bm]);
S3=series(S1,S2);%串連
S4=feedback(S3,K,-1);%回授
S5=tf([1],[1 0]);
S6=series(S4,S5);


S7=tf([Kd Kp Ki],[1 0]);
S8=series(S7,S6);

sys1=feedback(S8,1,-1);

step(sys1,0.01);
